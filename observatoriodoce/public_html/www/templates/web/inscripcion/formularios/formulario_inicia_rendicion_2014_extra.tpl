 <script type="text/javascript"> 
 
	 
 	 
	function enviarFormulario()
	{ 
		if(!emptyCheckValue('form_inicia_rendicion_fecha'))
		{
			showAlert('Debe seleccionar al menos una fecha de participacion');
			return fale;
		}
		   
		 enviarFormularioBase();
	}
	
	function revisarFecha()
	{
		var name = 'form_inicia_rendicion_fecha';
		var seleccion = '';
		var x = document.main.elements;	
		for (var i=0 ; i < x.length ; i++)
		{			 
			if(x[i].name == name && x[i].checked)
			{
				seleccion = x[i].value;		
				break;						
			}
		} 
		name = 'form_inicia_rendicion_fecha_extra';
		
		for (var i=0 ; i < x.length ; i++)
		{			 
			if(x[i].name == name )
			{					
				x[i].disabled = false;  
				if( x[i].value == seleccion)
				{
					x[i].checked = false;		
					x[i].disabled = true; 
				}
			}
		} 
	}
	
	 
	 </script>

